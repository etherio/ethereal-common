{
    "name": "etherio/ethereal",
    "description": "The Ethereal Framework by Etherio",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "etherio",
        "ethereal",
        "framework"
    ],
    "home": "https://github.com/etherio/ethereal-common",
    "authors": [
        {
            "name": "SHIN MAUNG",
            "email": "ethereal97@gmail.com"
        }
    ],
    "require": {
        "php": "*"
    },
    "autoload": {
        "psr-4": {
            "Ethereal\\Common\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload-dev": {
        "psr-4": {
            "Ethereal\\Common\\": "src/"
        }
    },
    "scripts": {
        "cs": "vendor/bin/phpcs",
        "fix": "vendor/bin/php-cs-fixer fix",
        "test": "vendor/bin/phpunit --verbose"
    }
}
